{
	"presets": [
		["@babel/preset-env", {
			"targets": {
				"browsers": ["last 2 versions", "safari >= 7"]
			},
			"useBuiltIns": "usage",
			"corejs": 3
		}],
		"@babel/preset-typescript"
	],
	"plugins": [[
		"@babel/plugin-transform-runtime",
		{
			"corejs": {
				"version": 3,
				"proposals": true
			},
			"helpers": true,
			"regenerator": true
		}
	]]
}